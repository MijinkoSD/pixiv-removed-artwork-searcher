{
  "name": "Pixivの削除されたブックマーク追跡ツール",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Pixivのブックマークにある削除済み・非表示イラストの検索ボタンを表示します。",
  "content_scripts": [
    {
      "matches": [
        "https://www.pixiv.net/users/*/bookmarks/*"
      ],
      "run_at": "document_idle",
      "js": [
        "./dist/main.mjs"
      ],
      "css": [
        "./dist/main.css"
      ]
    }
  ]
}
